		<Arguments>commit --acd "%project_path%" --pause</Arguments>
		<ToolName>Commit</ToolName>
		<StatusBarText>Commit to Git</StatusBarText>
		<ToolbarIcon IconIndex="0">@@OUTDIR@@\Assets\commit.bmp</ToolbarIcon>
