		<StatusBarText>Launch l5xgit.exe</StatusBarText>
		<ToolbarIcon IconIndex="0">@@OUTDIR@@\Assets\pull.bmp</ToolbarIcon>
	</CustomTool>
	<CustomTool>
		<Command>@@OUTDIR@@\l5xgit.exe</Command>
